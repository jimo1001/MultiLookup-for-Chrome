{
  "default_locale": "en",
  "name": "__MSG_ext_name__",
  "manifest_version": 2,
  "version": "0.6.4.1",
  "description": "__MSG_ext_description__",
  "icons": {
    "256": "img/icon256.png",
    "128": "img/icon128.png",
    "64": "img/icon64.png",
    "32": "img/icon32.png",
    "16": "img/icon16.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "background",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "all_frames": false,
      "js": [
        "js/multilookup.js"
      ],
      "css": [
        "css/multilookup.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "options_page": "html/options.html",
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
    "default_icon": "img/popup.png",
    "default_title": "MultiLookup",
    "default_popup": "html/popup.html"
  }
}
